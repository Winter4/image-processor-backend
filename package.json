{
  "name": "image-processor-backend",
  "version": "0.1",
  "description": "",
  "main": "",
  "scripts": {
    "update-packages": "sh ./scripts/update-packages.sh",
    "api:dev": "cross-env NODE_ENV=development npx nodemon source/api.ts | pino-pretty --translateTime",
    "test": "mocha --config ./tests/mocha.config.js --exit",
    "compile-wasm:single": "emcc source/api/image/image.wasm-processor/singlethread/singlethread.c -o source/api/image/image.wasm-processor/singlethread/singlethread.js -s EXPORTED_FUNCTIONS=\"['_malloc', '_free', '_processImage']\" -s EXPORT_NAME='imager_singlethread' -s MODULARIZE=1 -s TOTAL_MEMORY=2048MB",
    "compile-wasm:multi": "emcc source/api/image/image.wasm-processor/multithread/multithread.c -o source/api/image/image.wasm-processor/multithread/multithread.js -s EXPORTED_FUNCTIONS=\"['_malloc', '_free', '_processImage']\" -s EXPORT_NAME='imager_multithread' -s MODULARIZE=1 -s TOTAL_MEMORY=2048MB -pthread -s PTHREAD_POOL_SIZE=4 -s PTHREAD_POOL_SIZE_STRICT=1",
    "compile-wasm": "npm run compile-wasm:single && npm run compile-wasm:multi",
    "tsc": "sh ./scripts/compile-production.sh",
    "benchmark:build": "docker compose -f ./docker-compose.benchmark.yml build app",
    "benchmark:up": "docker compose -f ./docker-compose.benchmark.yml -p image-benchmark up --attach k6",
    "benchmark:start": "docker compose -f ./docker-compose.benchmark.yml -p image-benchmark start",
    "benchmark:stop": "docker compose -f ./docker-compose.benchmark.yml -p image-benchmark stop",
    "benchmark:down": "docker compose -f ./docker-compose.benchmark.yml -p image-benchmark down --volumes",
    "benchmark:restart": "npm run benchmark:down && npm run benchmark:up",
    "test:stop": "docker compose -f ./docker-compose.benchmark.yml -p image-benchmark stop k6"
  },
  "author": "Winter4 <mywinter4@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "ioredis": "^5.4.1",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^9.0.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "npm-check-updates": "^17.1.0",
    "should": "^13.2.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}